{% extends 'base.html.twig' %}

{% block title %}Création d'une catégorie
{% endblock %}

{% block body %}
	<h1>Créer une catégorie</h1>
	{{form_start(formView)}}
	{{form_errors(formView)}}
	<div class="row">
		<div class="col-4">
			{{form_widget(formView)}}
		</div>
	</div><br>
	<button type="submit" class="btn btn-primary">
		<i class="fas fa-save"></i>
		Enregistrer</button>
	{{form_end(formView)}}
{% endblock %}
